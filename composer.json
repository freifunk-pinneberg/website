{
  "name": "freifunk-pinneberg/website",
  "description": "Freifunk Pinneberg Website",
  "license": "CC-BY-NC-4.0",
  "repositories": [
    {
      "type": "composer",
      "url": "https://composer.typo3.org/"
    },
    {
      "type": "vcs",
      "url": "https://github.com/freifunk-pinneberg/typo3-tx_ffpi_nodecounter/"
    },
    {
      "type": "vcs",
      "url": "https://github.com/freifunk-pinneberg/typo3-tx_ffpi_theme/"
    },
    {
      "type": "vcs",
      "url": "https://github.com/freifunk-pinneberg/typo3-tx_ffpi_node_updates/"
    },
    {
      "type": "vcs",
      "url": "https://github.com/freifunk-pinneberg/website-vm/"
    },
    {
      "type": "vcs",
      "url": "https://github.com/TYPO3-extensions/extension_builder.git"
    }
  ],
  "config": {
    "vendor-dir": "vendor",
    "bin-dir": "bin",
    "web-dir": "web",
    "platform": {
      "php": "7.1"
    }
  },
  "minimum-stability": "beta",
  "require": {
    "typo3/cms": "~8.7",
    "typo3-ter/realurl": "2.*",
    "typo3-ter/cal": "^1.11",
    "sjbr/static-info-tables": "^6.7",
    "ffpi/ffpi-theme": "dev-feature/v2",
    "ffpi/node-updates": "dev-feature/typo3v9",
    "ffpi/nodecounter": "dev-feature/typo3v9",
    "derhansen/sf_yubikey": "^2.2",
    "ext-curl": "*"
  },
  "require-dev": {
    "phing/phing": "2.9.*",
    "freifunk-pinneberg/website-vm": "dev-master"
  },
  "extra": {
    "typo3/cms": {
      "cms-package-dir": "{$vendor-dir}/typo3/cms",
      "web-dir": "web"
    },
    "installer-paths": {
      "_vm": ["freifunk-pinenberg/website-vm"]
    }
  },
  "scripts": {
    "post-update-cmd": [
    ],
    "post-install-cmd": [
    ]
  },
  "autoload": {
    "psr-4": {
      "FFPI\\FfpiNodecounter\\": "web/typo3conf/ext/ffpi_nodecounter/Classes",
      "FFPI\\FfpiRouter\\": "web/typo3conf/ext/ffpi_router/Classes"
    }
  }
}
