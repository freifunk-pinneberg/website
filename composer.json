{
  "name": "freifunk-pinneberg/website",
  "description": "Freifunk Pinneberg Website",
  "license": "CC-BY-NC-4.0",
  "repositories": [
    {
      "type": "composer",
      "url": "https://composer.typo3.org/"
    },
    {
      "type": "git",
      "url": "git@github.com:freifunk-pinneberg/typo3-tx_ffpi_nodecounter.git"
    },
    {
      "type": "git",
      "url": "git@github.com:freifunk-pinneberg/typo3-tx_ffpi_theme.git"
    },
    {
      "type": "git",
      "url": "git@github.com:freifunk-pinneberg/typo3-tx_ffpi_helgo_theme.git"
    },
    {
      "type": "git",
      "url": "git@github.com:freifunk-pinneberg/typo3-tx_ffpi_node_updates.git"
    },
    {
      "type": "git",
      "url": "git@github.com:freifunk-pinneberg/typo3-tx_ffpi_firmware_list.git"
    }
  ],
  "config": {
    "vendor-dir": "vendor",
    "bin-dir": "bin",
    "web-dir": "web",
    "platform": {
      "php": "8.0"
    },
    "allow-plugins": {
      "typo3/class-alias-loader": true,
      "typo3/cms-composer-installers": true
    }
  },
  "minimum-stability": "beta",
  "require": {
    "typo3/cms-composer-installers": "3.*",
    "typo3/cms-core": "11.5.*",
    "typo3/cms-base-distribution": "11.*",
    "typo3/cms-recycler": "11.*",
    "typo3/cms-filemetadata": "11.*",
    "typo3/cms-workspaces": "11.*",
    "typo3/cms-backend": "11.*",
    "typo3/cms-beuser": "11.*",
    "typo3/cms-redirects": "11.*",
    "typo3/cms-linkvalidator": "11.*",
    "typo3/cms-lowlevel": "11.*",
    "typo3/cms-lang": "11.*",
    "typo3/cms-seo": "11.*",
    "typo3/cms-scheduler": "11.*",
    "ffpi/ffpi-theme": "dev-feature/typo3v11",
    "ffpi/ffpi-helgo-theme": "dev-feature/typo3-11",
    "ffpi/firmware-list": "dev-feature/typo3-11",
    "ffpi/node-updates": "dev-feature/typo3-11",
    "ffpi/nodecounter": "dev-feature/typo3-11",
    "friendsoftypo3/extension-builder": "*",
    "helhum/typo3-console": "*",
    "ext-curl": "*",
    "ext-openssl": "*",
    "ext-json": "*",
    "netresearch/rte-ckeditor-image": "^11.0",
    "lochmueller/staticfilecache": "^12.4"
  },
  "require-dev": {
    "deployer/deployer": "6.9.*",
    "rector/rector": "0.13.4",
    "michielroos/typo3scan": "^1.7",
    "saschaegerer/phpstan-typo3": "*",
    "phpstan/phpstan": "*",
    "helmich/typo3-typoscript-lint": "^2.5"
  },
  "extra": {
    "typo3/cms": {
      "cms-package-dir": "{$vendor-dir}/typo3/cms",
      "web-dir": "web"
    }
  },
  "scripts": {
    "post-update-cmd": [
    ],
    "post-install-cmd": [
    ]
  },
  "autoload": {
    "psr-4": {
    }
  }
}
